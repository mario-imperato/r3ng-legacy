<action path="/org/r3/console/home"
    className="org.r3.system.struts.GWFActionMapping"            
    type="org.r3.system.struts.R3Manager"
    parameter="home" >

    <!--
    scope="request"
    name="loginForm"
    validate="true"
    input="/security/signin.jsp">-->
       <set-property property="sessionBehaviour"   value="required"     />
       <set-property property="functionId"         value="console"      />    
       <set-property property="functionalAreaId"   value="home"         />           
       <set-property property="enforceTimeToken"   value="false"        />           
       
       <set-property property="sessionRequiredFailureForward"        value="ConsoleSessionRequiredFailure"  />
       <set-property property="authorizationRequiredFailureForward"  value="ConsoleSessionRequiredFailure"  />
       
       <forward name="YachtClubSiteHome"      path="/org/r3/console/home/yachtclubsite/home.jsp"      redirect="false" />
       <forward name="RegattaSiteHome"        path="/org/r3/console/home/site/home.jsp"               redirect="false" />        
       <forward name="DefaultSiteHome"        path="/org/r3/console/home/defaultsite/home.jsp"        redirect="false" />                         
       <forward name="SpeedSailsHome"         path="/org/r3/console/home/speedsails/home.jsp"         redirect="false" />  
       <forward name="WorkshopHome"           path="/org/r3/console/home/workshop/home.jsp"           redirect="false" />
       <forward name="MasterSiteHome"         path="/org/r3/console/home/mastersite/home.jsp"         redirect="false" />
       <forward name="ITILServiceSiteHome"    path="/org/r3/console/home/itilsite/home.jsp"           redirect="false" />
       <forward name="ShakeusHome"            path="/org/r3/console/home/shakeus/home.jsp"            redirect="false" />
       <forward name="ShakeusNeighbHome"      path="/org/r3/console/home/shakeusneighb/home.jsp"      redirect="false" />          
       <forward name="ShakeusShopHome"        path="/org/r3/console/home/shkshop/home.jsp"            redirect="false" />
</action>

